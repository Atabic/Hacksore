{
  "name": "hacksore",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "start": "concurrently npm:dev npm:emulator",
    "start:all": "concurrently npm:dev:all npm:emulator",
    "emulator": "firebase emulators:start --import seed",
    "dev": "turbo run start --filter=web",
    "dev:all": "turbo run start",
    "build": "turbo run build",
    "build:web": "turbo run build --filter=web",
    "storybook": "turbo run storybook --filter=web-storybook",
    "lint": "turbo run lint",
    "deploy": "vercel deploy"
  },
  "devDependencies": {
    "concurrently": "^7.3.0",
    "firebase-tools": "^11.15.0",
    "prettier": "^2.7.1",
    "turbo": "^1.4.5",
    "vercel": "^28.4.12"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
