{
  "compilerOptions": {
    "outDir": "dist",
    "rootDir": "src",
    "module": "esnext",
    "target": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node16",
    "resolveJsonModule": true,
    "strict": true,
    "skipLibCheck": true,
    "lib": ["esnext"],
    "typeRoots": ["node_modules/@types"]
  },
  "include": ["src/**/*"]
  
}
